#include <bits/stdc++.h>

int findDuplicate(vector<int> &arr, int n){
	// Write your code here.
	/*  map<int,bool> mp;

	for(auto x:arr){
		if(mp.find(x)== mp.end()){mp[x]=true;}
		else return x;
	}   */
	// another approach
	
	vector<bool> arr1(n,false);

	for(auto x:arr){
		if(arr1[x]==true) return x;
		else arr1[x]=true;
	}
}
